{
    "window.zoomLevel": 0,
    "workbench.activityBar.visible": false,
    "editor.minimap.enabled": false,
    "terminal.explorerKind": "external",
    "zenMode.fullScreen": true,
    "vim.insertModeKeyBindings": [
        {
            "before": ["j","j"],
            "after": ["<Esc>"]
        }
    ],
    "vim.otherModesKeyBindings": [
        {
            "before": ["n","n"],
            "after": ["<Esc>"]
        },
        {
            "before": ["o","o"],
            "after": ["o","<Exc>","k"]
        }
    ],
    "vim.useSystemClipboard": false,
    "vim.cmdLineInitialColon": true,
    "vim.disableExtension": false,
    "workbench.statusBar.feedback.visible": false,
    "workbench.colorTheme": "Charcoal Oceanic Next",
    "editor.renderLineHighlight": "gutter",
    "workbench.tree.horizontalScrolling": false,
    "vim.statusBarColorControl": false,
    "vim.showmodename": true,
    "editor.renderIndentGuides": false,
    "editor.colorDecorators": false,
    "workbench.colorCustomizations": {
       "[Charcoal Oceanic Next]": {
           "editorRuler.foreground": "#163824",
           "editor.background": "#191919",
           "statusBar.border": "#163824",
           "statusBar.background": "#191919",
           "sideBar.border": "#163824",
           "sideBar.background": "#191919"
       }
    },
    "editor.rulers": [
        0
    ],
    "editor.roundedSelection": false,
    "[javascriptreact]": {
        
    },
    "workbench.iconTheme": null,
    "editor.renderWhitespace": "none",
    "editor.renderControlCharacters": false,
    "editor.glyphMargin": false,
    "scm.diffDecorationsGutterWidth": 2,
    "scm.diffDecorations": "gutter",
    "editor.formatOnSaveTimeout": 750,
    "editor.fontFamily": "'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.fontLigatures": false,
}